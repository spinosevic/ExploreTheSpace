export material aluminum_capsule_mat(*)
  = adobe::mtl::standard(
    opacity : float(1.0),
    baseColor : adobe::util::color_texture( texture_2d("apollo_lunar_module/apollo_lunar_module_aluminum_capsule_mat_BaseColor.png", ::tex::gamma_srgb) ),
    translucence : float(0.0),
    glow : float(0.0),
    indexOfRefraction : float(1.6),
    density : float(0.0),
    roughness : adobe::util::float_texture( texture_2d("apollo_lunar_module/apollo_lunar_module_aluminum_capsule_mat_Roughness.png", ::tex::gamma_linear) ),
    normal : adobe::util::normal_texture( texture_2d("apollo_lunar_module/apollo_lunar_module_aluminum_capsule_mat_Normal.png", ::tex::gamma_linear) ),
    interiorColor : color(1.0, 1.0, 1.0),
    metallic : adobe::util::float_texture( texture_2d("apollo_lunar_module/apollo_lunar_module_aluminum_capsule_mat_Metallic.png", ::tex::gamma_linear) ),
    height : float(0.5),
    heightScale : float(1.0)
    );

